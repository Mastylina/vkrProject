

{% block body %}
    <h1 align="center">Report</h1>

    <table class="table">
        <thead>
        <tr>
            <th>Название услуги</th>
            <th>Описание</th>
        </tr>
        </thead>
        <tbody>
        {% for service in services %}
            <tr>
                <td>{{ service.name }}</td>
                <td>{{ service.description }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

{% endblock %}
